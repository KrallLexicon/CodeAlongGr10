
@{
    ViewData["Title"] = "AddCarToPerson";
}
@if(ViewBag.Message != null)
{
    <p>@ViewBag.Message</p>
}

<h1>Add car to person</h1>

<form asp-action="AddCarToPerson" method="post">
    <div class="form-group">
        <label>Person: </label>
        <select name="id" class="form-control" asp-items="@ViewBag.People"></select>
    </div> <div class="form-group">
        <label>Car: </label>
        <select name="regnumber" class="form-control" asp-items="@ViewBag.Cars"></select>
    </div>
    <input type="submit" value="Add owner to car" class="btn btn-primary" />
</form>

